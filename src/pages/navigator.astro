<pre></pre>
<script is:inline>
  window.addEventListener("load", () => {
    const navigator = window.navigator;
    const navigatorProps = {};
    Object.keys(Object.getPrototypeOf(navigator)).forEach((prop) => {
      try {
        navigatorProps[prop] = navigator[prop];
      } catch (e) {
        navigatorProps[prop] = `Access denied: ${e.message}`;
      }
    });

    const stringifiedNavigator = JSON.stringify(navigatorProps, null, 2);

    const pre = document.querySelector("pre");

    pre.innerText = stringifiedNavigator;
  });
</script>
